<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@android:color/transparent"
    android:orientation="vertical" 
    >
	
    
    <!--第一行 -->	
    <LinearLayout 
        style="@style/fwh"
        android:layout_marginLeft="5dip"
        android:layout_marginRight="5dip"
        >
        <LinearLayout
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            >
            <FrameLayout
			    android:layout_width="120dip"
			    android:layout_height="128dip"
			    android:background="@color/transparent"
			    android:orientation="vertical"
			    android:padding="0dip"
			     >
				<ImageView
				    android:id="@+id/iv_a_bg"
				    android:layout_height="128dip"
				    android:layout_width="120dip"
				    android:background="@drawable/sixmsg"
				    android:padding="0dip"
				    />
			    
			    <ImageView 
			        android:id="@+id/iv_a"
			        android:layout_height="80dip"
				    android:layout_width="80dip"
				    android:background="@color/transparent"
				    android:src="@drawable/ic_launcher"
				    android:layout_gravity="center"
				    />
			</FrameLayout>
        </LinearLayout>
        
        <LinearLayout 
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            >
             <FrameLayout
			    android:layout_width="120dip"
			    android:layout_height="128dip"
			    android:background="@color/transparent"
			    android:orientation="vertical"
			    android:padding="0dip"
			     >
				<ImageView 
				    android:id="@+id/iv_b_bg"
				    android:layout_height="128dip"
				    android:layout_width="120dip"
				    android:background="@drawable/sixmsg"
				    android:padding="0dip"
				    />
			    
			    <ImageView 
			        android:id="@+id/iv_b"
			        android:layout_height="80dip"
				    android:layout_width="80dip"
				    android:src="@drawable/ic_launcher"
				    android:layout_gravity="center"
				    />
			</FrameLayout>
        </LinearLayout>
        <LinearLayout 
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            >
            <FrameLayout
			    android:layout_width="120dip"
			    android:layout_height="128dip"
			    android:background="@color/transparent"
			    android:orientation="vertical"
			    android:padding="0dip"
			     >
				<ImageView 
				    android:id="@+id/iv_c_bg"
				    android:layout_height="128dip"
				    android:layout_width="120dip"
				    android:background="@drawable/sixmsg"
				    android:padding="0dip"
				    />
			    
			    <ImageView 
			        android:id="@+id/iv_c"
			        android:layout_height="80dip"
				    android:layout_width="80dip"
				    android:src="@drawable/ic_launcher"
				    android:layout_gravity="center"
				    />
			</FrameLayout>
        </LinearLayout>
        
    </LinearLayout>
    
    
    
    
    <!--第二行 -->	
    
     <LinearLayout 
        style="@style/fwh"
        android:layout_marginLeft="5dip"
        android:layout_marginRight="5dip"
        android:layout_marginTop="95dip"
        >
         <LinearLayout
            android:id="@+id/ll_left"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="0.5"
            >
           <ImageView 
               android:layout_height="128dip"
               android:layout_width="60dip"
               android:background="@drawable/six_right"
               />
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            >
             <FrameLayout
			    android:layout_width="120dip"
			    android:layout_height="128dip"
			    android:background="@color/transparent"
			    android:orientation="vertical"
			    android:padding="0dip"
			     >
				<ImageView
				    android:id="@+id/iv_d_bg" 
				    android:layout_height="128dip"
				    android:layout_width="120dip"
				    android:background="@drawable/sixmsg"
				    android:padding="0dip"
				    />
			    
			    <ImageView 
			        android:id="@+id/iv_d"
			        android:layout_height="80dip"
				    android:layout_width="80dip"
				    android:src="@drawable/ic_launcher"
				    android:layout_gravity="center"
				    />
			</FrameLayout>
        </LinearLayout>
        
        <LinearLayout 
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            >
             <FrameLayout
			    android:layout_width="120dip"
			    android:layout_height="128dip"
			    android:background="@color/transparent"
			    android:orientation="vertical"
			    android:padding="0dip"
			     >
				<ImageView
				    android:id="@+id/iv_e_bg" 
				    android:layout_height="128dip"
				    android:layout_width="120dip"
				    android:background="@drawable/sixmsg"
				    android:padding="0dip"
				    />
			    
			    <ImageView 
			        android:id="@+id/iv_e"
			        android:layout_height="80dip"
				    android:layout_width="80dip"
				    android:src="@drawable/ic_launcher"
				    android:layout_gravity="center"
				    />
			</FrameLayout>
        </LinearLayout>
        <LinearLayout 
             android:id="@+id/ll_right"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="0.5"
            >
           <ImageView 
               android:layout_height="128dip"
               android:layout_width="60dip"
               android:background="@drawable/six_left"
               />
        </LinearLayout>
        
    </LinearLayout>
    
   
</FrameLayout>